ta }\r\n    updateForm[e.target.name] =\r\n      e.target.type === 'file' ? e.target.files[0] : e.target.value\r\n    setFormData(updateForm)\r\n  }\r\n\r\n  // UPDATE SUBSCRIPTIONS\r\n  const [subscriptions, setSubscriptions] = useState('')\r\n  const updateSubscriptions = async () => {\r\n    try {\r\n      const channel = await API(`/subscribtion`)\r\n      setSubscriptions(channel.data.data.subscribtion)\r\n    } catch (err) {\r\n      console.log(err)\r\n    }\r\n  }\r\n\r\n  return (\r\n    <div className=\"home\">\r\n      <MainComponent\r\n        updateSubscription={updateSubscriptions}\r\n        subscriptions={subscriptions}\r\n      />\r\n      <div className=\"container-page\">\r\n        <h2 className=\"edit-channel\">Edit Channel</h2>\r\n        <div className=\"input-wrapper\">\r\n          <div className=\"input-title\">\r\n            <Input\r\n              placeholder=\"Channel Name\"\r\n              value={channelName === '' ? channel.channelName : channelName}\r\n              onChange={handleChangeText}\r\n              name=\"channelName\"\r\n            />\r\n          </div>\r\n          <div className=\"input-attach\">\r\n            <InputFile\r\n              height={55}\r\n              label={thumbnail === '' ? 'Upload Thumbnail' : thumbnail.name}\r\n              icon={Photo}\r\n              onChange={handleChangeText}\r\n              name=\"thumbnail\"\r\n              type=\"file\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <TextArea\r\n          placeholder=\"Description\"\r\n          onChange={handleChangeText}\r\n          value={description === '' ? channel.description : description}\r\n          name=\"description\"\r\n          height={177}\r\n        />\r\n        <Gap height={12} />\r\n        <div className=\"input-video\">\r\n          <InputFile\r\n            height={55}\r\n            label={photo === '' ? 'Upload Photo' : photo.name}\r\n            icon={Photo}\r\n            onChange={handleChangeText}\r\n            name=\"photo\"\r\n            type=\"file\"\r\n          />\r\n        </div>\r\n        <Gap height={45} />\r\n        <Button\r\n          title=\"Save\"\r\n          onClick={handleSubmit}\r\n          isLoading={isUploading ? true : false}\r\n          isDisabled={isUploading ? true : false}\r\n        />\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default EditChannel\r\n","H:\\2. WEB DEV\\DUMBWAYS BATCH19\\wayshub-fullstack\\client\\src\\pages\\404\\index.js",[],"H:\\2. WEB DEV\\DUMBWAYS BATCH19\\wayshub-fullstack\\client\\src\\components\\molecules\\MyVideos\\Description.js",[],"H:\\2. WEB DEV\\DUMBWAYS BATCH19\\wayshub-fullstack\\client\\src\\components\\molecules\\MyVideos\\index.js",["418"],"// React Packages\r\nimport { useEffect, useState } from 'react'\r\n\r\n// API\r\nimport { API } from '../../../config/api'\r\n\r\n// Components\r\nimport Thumbnail from '../Thumbnail'\r\n\r\nconst MyVideos = ({ user }) => {\r\n  const [videos, setVideos] = useState([])\r\n\r\n  const fetchVideos = async () => {\r\n    try {\r\n      const response = await API(`/channel/${user}`)\r\n\r\n      setVideos(response.data.data.channel.videos)\r\n    } catch (err) {\r\n      console.log(err)\r\n    }\r\n  }\r\n\r\n  useEffect(() => {\r\n    fetchVideos()\r\n  }, [])\r\n\r\n  // Delete Video\r\n  const handleDeleteVideo = async () => {\r\n    try {\r\n      const response = await API(`/channel/${user}`)\r\n      setVideos(response.data.data.channel.videos)\r\n    } catch (err) {\r\n      console.log(err)\r\n    }\r\n  }\r\n\r\n  return (\r\n    <div className=\"videos\">\r\n      <div className=\"thumbnail\">\r\n        {videos.map((item) => (\r\n          <div className=\"thumbnail-wrapper\" key={item.id}>\r\n            <Thumbnail\r\n              id={item.id}\r\n              idChannel={user}\r\n              img={item.thumbnail}\r\n              title={item.title}\r\n              views={item.viewcount}\r\n              date={item.cre