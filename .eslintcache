ssword = async (e) => {\r\n    e.preventDefault()\r\n    const body = JSON.stringify({ email, password })\r\n\r\n    const config = {\r\n      headers: {\r\n        'Content-Type': 'application/json',\r\n      },\r\n    }\r\n    try {\r\n      const response = await API.patch(`/channel/${id}`, body, config)\r\n      dispatch({\r\n        type: 'LOGIN',\r\n        payload: response.data.data.channel,\r\n      })\r\n      \r\n      setAuthToken(response.data.data.channel.token)\r\n\r\n      setSuccessMessage(true)\r\n    } catch (err) {\r\n      console.log(err)\r\n    }\r\n  }\r\n\r\n  return (\r\n    <div className=\"reset\">\r\n      <div\r\n        className=\"reset-password\"\r\n        style={{ display: successMessage ? 'none' : '' }}\r\n      >\r\n        <form>\r\n          <h1 className=\"title\">Reset Password</h1>\r\n          <span\r\n            style={{\r\n              display: errorEmail ? '' : 'none',\r\n              position: 'absolute',\r\n              color: 'red',\r\n              top: 85,\r\n            }}\r\n          >\r\n            Email anda salah!\r\n          </span>\r\n          <div className=\"input-email\">\r\n            <Input\r\n              id=\"email\"\r\n              placeholder=\"Input your email\"\r\n              name=\"email\"\r\n              value={email}\r\n              onChange={handleChangeText}\r\n              required\r\n            />\r\n            <Gap height={20} />\r\n          </div>\r\n          <div style={{ display: isContinue ? 'none' : '' }}>\r\n            <Button title=\"Continue\" onClick={handleContinue} />\r\n          </div>\r\n          <div\r\n            className=\"input-password\"\r\n            style={{ display: isContinue ? '' : 'none' }}\r\n          >\r\n            <Input\r\n              id=\"password\"\r\n              placeholder=\"Your new password\"\r\n              type=\"password\"\r\n              name=\"password\"\r\n              value={password}\r\n              onChange={handleChangeText}\r\n            />\r\n            <Gap height={20} />\r\n          </div>\r\n          <div\r\n            className=\"input-confirm-password\"\r\n            style={{ display: isContinue ? '' : 'none' }}\r\n          >\r\n            <Input\r\n              id=\"confirmPassword\"\r\n              placeholder=\"Confirm new password\"\r\n              type=\"confirmPassword\"\r\n              name=\"confirmPassword\"\r\n            />\r\n            <Gap height={30} />\r\n          </div>\r\n          <div\r\n            className=\"button-reset\"\r\n            style={{ display: isContinue ? '' : 'none' }}\r\n          >\r\n            <Button title=\"Reset\" onClick={updatePassword} />\r\n          </div>\r\n        </form>\r\n      </div>\r\n      <div\r\n        className=\"success-message\"\r\n        style={{ display: successMessage ? '' : 'none' }}\r\n      >\r\n        <span>Changed Password Successfully</span>\r\n        <Gap height={20} />\r\n        <Button\r\n          title=\"Bring me to Home\"\r\n          onClick={() => history.push('/home')}\r\n        />\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default ForgotPassword\r\n","H:\\2. WEB DEV\\DUMBWAYS BATCH19\\wayshub-fullstack\\client\\src\\components\\atoms\\progressBar\\index.js",[],"H:\\2. WEB DEV\\DUMBWAYS BATCH19\\wayshub-fullstack\\client\\src\\pages\\WatchLaterPage\\index.js",["434"],"H:\\2. WEB DEV\\DUMBWAYS BATCH19\\wayshub-fullstack\\client\\src\\components\\molecules\\ThumbnailWatchLater\\index.js",["435","436"],{"ruleId":"437","replacedBy":"438"},{"ruleId":"439","replacedBy":"440"},{"ruleId":"437","replacedBy":"441"},{"ruleId":"439","replacedBy":"442"},{"ruleId":"437","replacedBy":"443"},{"ruleId":"439","replacedBy":"444"},{"ruleId":"445","severity":1,"message":"446","line":10,"column":10,"nodeType":"447","messageId":"448","endLine":10,"endColumn":15},{"ruleId":"449","severity":1,"message":"450","line":21,"column":59,"nodeType":"451","messageId":"452","endLine":2